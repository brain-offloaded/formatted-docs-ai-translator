{
  "name": "formatted-docs-ai-translator",
  "version": "0.1.0",
  "description": "Formatted Docs such as Json Translation Tool using Artificial Intelligence",
  "main": "dist/main.js",
  "author": "leejs1030",
  "engines": {
    "node": "22.14.0"
  },
  "scripts": {
    "start": "npm run build && electron ./dist/main.js",
    "build": "tsc && tsc-alias && webpack",
    "postinstall": "electron-rebuild && electron-builder install-app-deps",
    "watch": "concurrently \"tsc -w\" \"webpack --watch\"",
    "watch-ts": "tsc -w",
    "start:dev": "npm run build && concurrently \"npm run watch\" \"nodemon --watch dist --exec electron ./dist/main.js\"",
    "clean": "rimraf dist",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:unit": "jest --testPathPattern=src/",
    "package:win": "npm run build && electron-builder build --win",
    "package:linux": "npm run build && electron-builder build --linux",
    "prisma:studio": "prisma db pull && prisma studio",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:create": "npm run typeorm migration:create -- -n",
    "migration:run": "npm run typeorm migration:run -- -d src/nest/db/typeorm/datasource.ts",
    "migration:revert": "npm run typeorm migration:revert -- -d src/nest/db/typeorm/datasource.ts",
    "migration:generate": "npm run typeorm migration:generate -- -d src/nest/db/typeorm/datasource.ts",
    "nest:generate": "nest generate",
    "nest:g:module": "nest generate module",
    "nest:g:controller": "nest generate controller",
    "nest:g:service": "nest generate service",
    "nest:g:provider": "nest generate provider",
    "nest:g:class": "nest generate class"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@google/generative-ai": "^0.24.0",
    "@mui/icons-material": "^6.4.8",
    "@mui/material": "^6.4.8",
    "@mui/system": "^6.4.8",
    "@nestjs/common": "^11.0.12",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.12",
    "@nestjs/cqrs": "^11.0.3",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/microservices": "^11.0.12",
    "@nestjs/testing": "^11.0.12",
    "@nestjs/typeorm": "^11.0.0",
    "@types/adm-zip": "^0.5.7",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@types/rfdc": "^1.2.0",
    "adm-zip": "^0.5.16",
    "better-sqlite3": "^11.9.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dataloader": "^2.2.3",
    "dotenv": "^16.4.5",
    "electron-dl": "^4.0.0",
    "electron-updater": "^6.6.2",
    "jszip": "^3.10.1",
    "limiter": "^2.1.0",
    "lru-cache": "^11.0.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "reflect-metadata": "^0.2.2",
    "rfdc": "^1.4.1",
    "rxjs": "^7.8.2",
    "tslib": "^2.8.1",
    "typeorm": "^0.3.20",
    "winston": "^3.17.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.7.1",
    "@nestjs/cli": "^11.0.5",
    "@types/better-sqlite3": "^7.6.12",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.27",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "concurrently": "^8.2.2",
    "css-loader": "^7.1.2",
    "electron": "^29.4.6",
    "electron-builder": "^24.13.3",
    "electron-rebuild": "^3.2.9",
    "electron-reloader": "^1.2.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "prisma": "^6.5.0",
    "rimraf": "^5.0.5",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.15",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  },
  "build": {
    "appId": "com.leejs1030.formatted-docs-ai-translator",
    "productName": "Formatted Docs AI Translator",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "package.json",
      "index.html"
    ],
    "asar": true,
    "win": {
      "target": [
        "zip"
      ],
      "icon": "assets/icon.ico"
    },
    "linux": {
      "target": [
        "zip"
      ],
      "icon": "assets/icon.ico"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "leejs1030",
        "repo": "formatted-docs-ai-translator"
      }
    ]
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "repo": "formatted-docs-ai-translator"
  }
}
